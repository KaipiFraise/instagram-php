{
    "name": "haridarshan/instagram-php",
    "description": "Implements the Instagram APIs specified by http://instagram.com/developer/",
    "keywords": ["instagram", "instagram-api", "php"],
    "type": "library",
    "license": "GNU GPL v3",
    "homepage": "https://github.com/haridarshan/instagram-php",
    "authors": [
        {
            "name": "Haridarshan Gorana",
            "email": "er.haridarshan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7"
    },
    "autoload": {
        "psr-4": {
            "Haridarshan\\Instagram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Haridarshan\\Instagram\\Tests\\": "tests",
            "GuzzleHttp\\Tests\\": "vendor/guzzlehttp/guzzle/tests"
        }
    }
}
